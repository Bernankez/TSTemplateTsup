{
  "name": "ts-template-tsup",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.3.1",
  "description": "",
  "author": {
    "name": "Cole",
    "email": "bernankez@qq.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/bernankez/TSTemplateTsup#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bernankez/TSTemplateTsup.git"
  },
  "bugs": "https://github.com/bernankez/TSTemplateTsup/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepublishOnly": "npm run build",
    "build": "tsup",
    "dev": "tsup --watch src/index.ts",
    "release": "bumpp && npm publish --access public",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^0.2.1",
    "@types/node": "^18.11.19",
    "bumpp": "^8.2.1",
    "changelogithub": "^0.12.7",
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vitest": "^0.28.4"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
